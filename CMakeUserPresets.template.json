{
   "version": 2,
   "configurePresets": [
      {
         "name": "default-debug",
         "displayName": "User Debug Config",
         "inherits": [ "llvm-template-preset" ],
         "cacheVariables": {
            "CMAKE_BUILD_TYPE": "Debug",
            "CMAKE_C_COMPILER": "/path/to/clang",
            "CMAKE_CXX_COMPILER": "/path/to/clang++"
         }
      }
   ]
}
